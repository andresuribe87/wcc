<div class="row">
  <div class="span6 offset3">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>User</th>
          <th>Score</th>
        </tr>
      </thead>

      <tbody>
        <% @rankings.each do |ranking| %>
          <tr>
            <td><%= link_to ranking.user.name, ranking.user %></td>
            <td><%= ranking.score %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <%= will_paginate @rankings %>
  </div>
</div>